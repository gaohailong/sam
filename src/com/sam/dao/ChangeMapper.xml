<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sam.dao.ChangeDao">
	<insert id="addChange" parameterType="com.sam.entity.AssetChange">
		insert into AssetChange(acid,auname,acdate,adname,acresult)
		 values(#{acid},#{auname},sysdate,#{adname},'已移交')
	</insert>
</mapper>
