<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sam.dao.InfoDao">
	<insert id="addInfo" parameterType="com.sam.entity.AssetInfo">
		insert into assethouse(aiid,aimodel,ainame,atname,ahname,atid,ahid,aidate,aiMeasuer,aiSupply,aiMoney,aiLife,aiRemark)
		 values(assetinfo_seq.nextval,#{aimodel},#{ainame},#{atname},#{ahname},#{atid},#{ahid},#{aidate},#{aiMeasuer},#{aiSupply},#{aiMoney},#{aiLife},#{aiRemark});
	</insert>
</mapper>
